@model IEnumerable<MyAnnonser.Models.Ads>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UniqueId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdHeadline)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdText)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.AdvertiserId)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.PublicationDates)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UniqueId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdHeadline)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdText)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.AdvertiserId)
        </td*@>
        <td>
            @Html.DisplayFor(modelItem => item.PublicationDates)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id= item.UniqueId}) |
            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.UniqueId },
             new {onclick="return confirm('Are sure you want to delete');"})    
        </td>
    </tr>
}

</table>
